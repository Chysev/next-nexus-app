{
  "name": "next-nexus-backend",
  "version": "0.1.0",
  "main": "src/server.ts",
  "author": "Chysev",
  "license": "MIT",
  "keywords": [
    "NodeJS",
    "Typescript",
    "Express",
    "Prisma",
    "Boilerplate",
    "Authorization",
    "Authentication",
    "JWT",
    "SESSION",
    "NEXTJS"
  ],
  "private": true,
  "scripts": {
    "dev": "npm run prisma:generate && cross-env NODE_ENV=development ts-node-dev --project tsconfig.dev.json -r tsconfig-paths/register --transpile-only --respawn --exit-child src/cluster.ts",
    "build:production": "npm run prisma:generate && tsc --build tsconfig.build.json && tsc-alias -p tsconfig.json",
    "production": "npm run prodEnv cross-env NODE_ENV=production node ./build/cluster.js",
    "clean:build": "rm -rf build",
    "check:types": "tsc --noEmit",
    "clean": "rm -rf node_modules coverage build .nyc_output",
    "devEnv": "dotenv -e .env.dev --",
    "prodEnv": "dotenv -e .env.prod --",
    "prisma": "npx prisma",
    "prisma:format": "npx prisma format",
    "prisma:generate": "npm run devEnv npx prisma generate",
    "prisma:db-push": "npm run devEnv npx prisma db push",
    "prisma:migrate": "npm run devEnv npx prisma migrate dev --name init",
    "prisma:seed": "npm run devEnv npx prisma db seed",
    "prisma:studio": "npm run devEnv npx prisma studio"
  },
  "dependencies": {
    "@prisma/client": "^5.18.0",
    "@prisma/extension-accelerate": "^1.1.0",
    "axios": "^1.7.2",
    "bcrypt": "^5.1.1",
    "chalk": "^5.3.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "cloudinary": "^2.4.0",
    "connect-redis": "^7.1.1",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "dotenv-cli": "^7.4.2",
    "ejs": "^3.1.10",
    "express": "^4.19.2",
    "express-session": "^1.18.0",
    "helmet": "^7.1.0",
    "jose": "^5.6.3",
    "jsonwebtoken": "^9.0.2",
    "kleur": "^4.1.5",
    "morgan": "^1.10.0",
    "multer": "^1.4.5-lts.1",
    "nocache": "^4.0.0",
    "nodemailer": "^6.9.14",
    "passport": "^0.7.0",
    "passport-google-oauth20": "^2.0.0",
    "passport-jwt": "^4.0.1",
    "passport-local": "^1.0.0",
    "path": "^0.12.7",
    "redis": "^4.7.0",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "url": "^0.11.3"
  },
  "devDependencies": {
    "@swc/core": "^1.7.0-nightly-20240713.1",
    "@swc/helpers": "^0.5.12",
    "@types/bcrypt": "^5.0.2",
    "@types/cookie-parser": "^1.4.7",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/express-session": "^1.18.0",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/morgan": "^1.9.9",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.10",
    "@types/nodemailer": "^6.4.15",
    "@types/passport": "^1.0.16",
    "@types/passport-jwt": "^4.0.1",
    "@types/redis": "^4.0.11",
    "@types/swagger-ui-express": "^4.1.6",
    "cross-env": "^7.0.3",
    "prisma": "^5.18.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsc-alias": "^1.8.10",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.3"
  }
}
