{
  "extends": "next",
  "rules": {
    "@next/next/no-page-custom-font": 1,
    "no-unused-vars": "off",
    "no-undef": "warn",
    "react-hooks/rules-of-hooks": "off",
    "react/jsx-no-duplicate-props": "error",
    "react/jsx-key": "error",
    "react/jsx-no-undef": ["error", { "allowGlobals": false }]
  },
  "globals": {
    "React": true
  }
}
